{
    "name": "huhgesbot",
    "manifest_version": 2,
    "version": "1.0",
    "description": "tweet ur searches",
    "icons": {"128": "icon128.png", "48": "icon48.png", "16": "icon16.png"},
    "background": {
        "scripts": [
            "chrome_ex_oauth.min.js",
            "background.min.js"
        ],
        "persistent": false
    },
    "web_accessible_resources": [
        "chrome_ex_oauth.html"
        ],
    "permissions": ["webNavigation", "tabs", "https://api.twitter.com/", "https://abs.twimg.com/", "https://*/*"],
    "content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self' https://abs.twimg.com"
}
