{
  "name": "tomhuhges-bot",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "tweet ur searches",
  "background": {
    "scripts": [
      "auth.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "chrome_ex_oauth.html"
  ],
  "permissions": [
    "webNavigation",
    "tabs",
    "https://api.twitter.com/*"
  ],
  "page_action": {
    "default_title": "tomhuhgesbot"
  }
}
